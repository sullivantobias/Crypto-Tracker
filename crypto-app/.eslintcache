ers/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/store/reducers/index.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/api/api.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/pages/News/news.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/header/header.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/pages/Home/home.js",["114"],"import React, { useState, useEffect } from 'react';\nimport { useSelector } from 'react-redux'\nimport { fetchMarkets } from '../../api/api'\nimport Listing from '../../components/listing/listing';\n\nimport './home.scss';\n\nconst Home = () => {\n    const [listingInfos, setListingInfos] = useState([]);\n    const [loadingList, setLoadingList] = useState(false);\n\n    const currencyData = useSelector(state => state.currency)\n\n    useEffect(() => {\n        listinghandler()\n    }, [currencyData]);\n\n    const listinghandler = async () => {\n        setLoadingList(false)\n\n        const listingMarkets = await fetchMarkets(currencyData.currency);\n\n        setLoadingList(true)\n        setListingInfos(listingMarkets);\n    }\n\n    return (\n        <div className='cmp-page-home'>\n            <Listing loadingList={loadingList} keys={['Name', 'Price', '24h', 'Market Cap', 'Volume', 'Circulating Supply', 'Last 7 Days']} datas={listingInfos} currency={currencyData.currency} />\n        </div>\n    );\n}\n\nexport default Home;","/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/listing/listing.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/store/reducers/currencyReducer.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/constants/api.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/currencySelector/currencySelector.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/listing/propTypes.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/store/actions/index.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/store/actions/currencyHandler.js",["115"],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/header/propTypes.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/currencySelector/propTypes.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/commons/loader/loader.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/pages/Currency/currency.js",["116"],"import React, { useState, useEffect, useMemo } from 'react';\nimport { useLocation } from \"react-router-dom\";\nimport { useSelector } from 'react-redux'\nimport { fetchCurrencyDetails, fetchCurrencyMarketChart } from '../../api/api';\nimport { Loader } from '../../components/commons/loader/loader';\nimport Basic from '../../components/details/basic';\nimport Chart from '../../components/details/chart/chart';\nimport Filter from '../../components/filter/filter';\n\nimport './currency.scss';\n\nconst Details = () => {\n    const { currency } = useSelector(state => state.currency)\n    let data = useLocation();\n\n    const [mounted, setMounted] = useState(false)\n    const [currencyDetails, setCurrencyDetails] = useState({})\n    const [currencyMarketChart, setCurrencyMarketChart] = useState([])\n    const [updating, setUpdating] = useState(true)\n    const [time, setTime] = useState('1')\n\n    const timesFilter = useMemo(() =>\n        <Filter onChangeValues={value => setTime(value)}\n            filters={[{ label: 'Day', value: 1 },\n            { label: 'Week', value: 7 },\n            { label: 'Month', value: 30 },\n            { label: 'Year', value: 365 },\n            { label: 'All', value: 'max' }]} />, []);\n\n    useEffect(() => {\n        if (!mounted) { detailsHandler(); setMounted(true) }\n        marketChartHandler()\n    }, [currency, time]);\n\n    const detailsHandler = async () => {\n        const details = await fetchCurrencyDetails(data.state.cryptoCurrency);\n        setCurrencyDetails(details);\n    }\n\n    const marketChartHandler = async () => {\n        setUpdating(true)\n        const mcData = await fetchCurrencyMarketChart(data.state.cryptoCurrency, currency, time);\n        setCurrencyMarketChart(mcData);\n        setUpdating(false)\n    }\n\n    return (\n        <div className='cmp-page-currency'>\n            { Object.keys(currencyDetails).length ? <Basic currency={currency} details={currencyDetails} /> : <Loader />}\n            {timesFilter}\n            { !updating && Object.keys(currencyMarketChart).length ? <Chart time={time} data={currencyMarketChart} /> : <Loader />}\n        </div>\n    );\n}\n\nexport default Details;","/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/details/basic.js",["117"],"import React from 'react';\nimport { IoIosLink, IoIosPricetag } from 'react-icons/io';\nimport Gauge from './gauge/gauge';\nimport { formatNumbers } from '../utils';\n\nimport './basic.scss';\n\nconst Basic = ({ details, currency }) => {\n    const getUrlParsed = url => {\n        const hN = new URL(url).hostname;\n        return <a target='_blank' href={url}><IoIosLink />{hN}</a>\n    }\n\n    const isGoingDown = price => {\n        if (price === 0) return\n        return price < 0 ? 'isDown' : 'isUp';\n    }\n\n    return (\n        <div className='cmp-details-basic'>\n            <div className='cmp-details-basic__marker'>\n                <img className='cmp-details-basic__marker--logo' src={details.image.small} alt={details.name} />\n                <div className='cmp-details-basic__marker__badges'>\n                    <div className='cmp-details-basic__marker__badges--rank'><span>Rank #{details.market_cap_rank}</span></div>\n                    <div className='cmp-details-basic__marker__badges--links'>{details.links.homepage.map((item, index) => item && <span key={index} className='cmp-details-basic__marker__badges--links--link'>{getUrlParsed(item)}</span>)}</div>\n                    <div className='cmp-details-basic__marker__badges--tags'>{details.categories.map((item, index) => <span key={index} className='cmp-details-basic__marker__badges--tags--tag'><IoIosPricetag />{item}</span>)}</div>\n                </div>\n            </div>\n            <div className='cmp-details-basic__prices'>\n                <h4 className='cmp-details-basic__prices--title'>{details.name} Price <span>({details.symbol})</span></h4>\n                <div className='cmp-details-basic__prices--price'> {formatNumbers(currency, details.market_data.current_price[currency])} </div>\n                <div className={`cmp-details-basic__prices--change ${isGoingDown(details.market_data.price_change_percentage_24h)}`} > {details.market_data.price_change_percentage_24h.toFixed(2)}%</div>\n                <div className='cmp-details-basic__prices__low-high'>\n                    <span className='cmp-details-basic__prices__low-high--low'>Low: <span className='value'>{formatNumbers(currency, details.market_data.low_24h[currency])}</span></span>\n                    <span className='cmp-details-basic__prices__low-high--high'>High: <span className='value'>{formatNumbers(currency, details.market_data.high_24h[currency])}</span></span>\n                    <span className='cmp-details-basic__prices__low-high__gauge'>\n                        <Gauge filled={details.market_data.current_price[currency]} total={details.market_data.high_24h[currency]} />\n                    </span>\n                </div>\n            </div>\n        </div >\n    );\n}\n\nexport default Basic;","/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/navigation/navigation.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/details/chart/chart.js",["118"],"import React from 'react';\n\nimport { Line } from 'react-chartjs-2';\n\nimport './chart.scss';\n\nconst Chart = ({ data, time }) => {\n    const generateDataPoints = () => {\n        let datas = { key: [], value: [] };\n\n        data && data.prices.map(d => {\n            datas.key = [...datas.key, formatDate(new Date(d[0]))]\n            datas.value = [...datas.value, d[1].toFixed(d[1] < 10000 ? 4 : 0)]\n        })\n\n        return datas;\n    }\n\n    const formatDate = date => {\n        let res = ''\n\n        switch (time) {\n            case '1':\n                let hours = date.getHours();\n                let minutes = date.getMinutes();\n\n                if (hours < 10) hours = \"0\" + hours;\n                if (minutes < 10) minutes = \"0\" + minutes;\n\n                res = \"\" + hours + \":\" + minutes;\n                break;\n\n            case '7':\n            case '30':\n                const days = ['Sun', 'Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat'];\n\n                res = `${date.getDate()}. ${days[date.getDay()]}`;\n                break;\n\n            case '365':\n                const months = ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'];\n                res = months[date.getMonth()];\n                break;\n\n            case 'max':\n                res = date.getFullYear()\n                break;\n\n            default:\n                break;\n        }\n\n        return res;\n    }\n\n    const chartOptions = {\n        legend: {\n            display: false\n        },\n\n        scales: {\n            yAxes: [\n                {\n                    ticks: {\n                        fontColor: \"white\",\n                        fontStyle: \"bold\"\n                    }\n                }\n            ],\n            xAxes: [\n                {\n                    ticks: {\n                        maxTicksLimit: (() => {\n                            if (time === '1' || time === '7' || '365') return 7;\n                            else if (time === '30') return 16\n                        })(),\n                        fontColor: \"white\",\n                        fontStyle: \"bold\",\n                        maxRotation: 0,\n                        minRotation: 0\n                    }\n                }\n            ]\n        },\n    }\n\n    const chartData = {\n        labels: generateDataPoints().key,\n        datasets: [\n            {\n                borderColor: '#8dc647',\n                pointBorderWidth: 1,\n                pointHoverRadius: 5,\n                pointHoverBackgroundColor: 'rgba(75,192,192,1)',\n                pointHoverBorderColor: 'rgba(220,220,220,1)',\n                pointHoverBorderWidth: 2,\n                pointRadius: 1,\n                pointHitRadius: 10,\n                data: generateDataPoints().value,\n            }\n        ]\n    };\n\n    return (\n        <div className='cmp-details-chart'>\n            <Line data={chartData} options={chartOptions} />\n        </div>\n    );\n}\n\nexport default Chart;","/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/details/gauge/gauge.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/utils/index.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/utils/formatNumbers.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/filter/filter.js",[],{"ruleId":"119","replacedBy":"120"},{"ruleId":"121","replacedBy":"122"},{"ruleId":"123","severity":1,"message":"124","line":23,"column":6,"nodeType":"125","endLine":23,"endColumn":8,"suggestions":"126"},{"ruleId":"123","severity":1,"message":"127","line":16,"column":8,"nodeType":"125","endLine":16,"endColumn":22,"suggestions":"128"},{"ruleId":"129","severity":1,"message":"130","line":15,"column":1,"nodeType":"131","endLine":18,"endColumn":2},{"ruleId":"123","severity":1,"message":"132","line":33,"column":8,"nodeType":"125","endLine":33,"endColumn":24,"suggestions":"133"},{"ruleId":"134","severity":1,"message":"135","line":11,"column":16,"nodeType":"136","endLine":11,"endCol