patch(allActions.currencyActions.fetchCurrencies(currencies))\n  }\n\n  const headerHandler = async () => {\n    const globalInfos = await fetchGlobalInfos();\n    const { active_cryptocurrencies,\n      market_cap_percentage,\n      market_cap_change_percentage_24h_usd\n    } = globalInfos.data\n\n    setHeaderInfos(\n      {\n        ...headerInfos,\n        Coins: active_cryptocurrencies,\n        \"24h Market Cap Change\": `${market_cap_change_percentage_24h_usd.toFixed(1)}%`,\n        'BTC  Dominance': `${market_cap_percentage.btc.toFixed(2)}%`\n      }\n    );\n  }\n\n  return (\n    <div className=\"App\">\n      <Router>\n        <Navigation links={[{ path: '/', title: 'Cryptocurrencies' }, { path: '/news', title: 'News' }]} />\n        <Header datas={headerInfos} />\n        <>\n          <Navigation isBurger={false} links={[{ path: '/', title: 'Cryptocurrencies' }, { path: '/news', title: 'News' }]} />\n          <Switch>\n            <Route exact path='/' component={Home} />\n            <Route path='/currencies' component={Currency} />\n            <Route path='/news' component={News} />\n          </Switch>\n        </>\n      </Router>\n    </div >\n  );\n}\n\nexport default App","/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/store/reducers/index.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/api/api.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/pages/News/news.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/header/header.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/pages/Home/home.js",["128"],"import React, { useState, useEffect } from 'react';\nimport { useSelector } from 'react-redux'\nimport { fetchMarkets } from '../../api/api'\nimport Listing from '../../components/listing/listing';\n\nimport './home.scss';\n\nconst Home = () => {\n    const [listingInfos, setListingInfos] = useState([]);\n    const [loadingList, setLoadingList] = useState(false);\n\n    const currencyData = useSelector(state => state.currency)\n\n    useEffect(() => {\n        listinghandler()\n    }, [currencyData]);\n\n    const listinghandler = async () => {\n        setLoadingList(false)\n\n        const listingMarkets = await fetchMarkets(currencyData.currency);\n\n        setLoadingList(true)\n        setListingInfos(listingMarkets);\n    }\n\n    return (\n        <div className='cmp-page-home'>\n            <Listing loadingList={loadingList} keys={['Name', 'Price', '24h', 'Market Cap', 'Volume', 'Circulating Supply', 'Last 7 Days']} datas={listingInfos} currency={currencyData.currency} />\n        </div>\n    );\n}\n\nexport default Home;","/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/listing/listing.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/store/reducers/currencyReducer.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/constants/api.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/currencySelector/currencySelector.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/listing/propTypes.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/store/actions/index.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/store/actions/currencyHandler.js",["129"],"const changeCurrency = currency => {\n    return {\n        type: \"CHANGE_CURRENCY\",\n        payload: currency\n    }\n}\n\nconst fetchCurrencies = currencies => {\n    return {\n        type: \"FETCH_CURRENCIES\",\n        payload: currencies\n    }\n}\n\nexport default {\n    changeCurrency,\n    fetchCurrencies\n}","/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/header/propTypes.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/currencySelector/propTypes.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/commons/loader/loader.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/pages/Currency/currency.js",["130"],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/details/basic.js",["131"],"import React from 'react';\nimport { IoIosLink, IoIosPricetag } from 'react-icons/io';\nimport { formatNumbers } from '../utils';\nimport Gauge from './gauge/gauge';\n\nimport './basic.scss';\n\nconst Basic = ({ details, currency }) => {\n    const getUrlParsed = url => {\n        const hN = new URL(url).hostname;\n        return <a target='_blank' href={url}><IoIosLink />{hN}</a>\n    }\n\n    const isGoingDown = price => {\n        if (price === 0) return\n        return price < 0 ? 'isDown' : 'isUp';\n    }\n\n    return (\n        <div className='cmp-details-basic'>\n            <div className='cmp-details-basic__marker'>\n                <img className='cmp-details-basic__marker--logo' src={details.image.small} alt={details.name} />\n                <div className='cmp-details-basic__marker__badges'>\n                    <div className='cmp-details-basic__marker__badges--rank'><span>Rank #{details.market_cap_rank}</span></div>\n                    <div className='cmp-details-basic__marker__badges--links'>{details.links.homepage.map((item, index) => item && <span key={index} className='cmp-details-basic__marker__badges--links--link'>{getUrlParsed(item)}</span>)}</div>\n                    <div className='cmp-details-basic__marker__badges--tags'>{details.categories.map((item, index) => <span key={index} className='cmp-details-basic__marker__badges--tags--tag'><IoIosPricetag />{item}</span>)}</div>\n                </div>\n            </div>\n            <div className='cmp-details-basic__prices'>\n                <h4 className='cmp-details-basic__prices--title'>{details.name} Price <span>({details.symbol})</span></h4>\n                <div className='cmp-details-basic__prices--price'> {formatNumbers(currency, details.market_data.current_price[currency])} </div>\n                <div className={`cmp-details-basic__prices--change ${isGoingDown(details.market_data.price_change_percentage_24h)}`} > {details.market_data.price_change_percentage_24h.toFixed(2)}%</div>\n                <div className='cmp-details-basic__prices__low-high'>\n                    <span className='cmp-details-basic__prices__low-high--low'>Low: <span className='value'>{formatNumbers(currency, details.market_data.low_24h[currency])}</span></span>\n                    <span className='cmp-details-basic__prices__low-high--high'>High: <span className='value'>{formatNumbers(currency, details.market_data.high_24h[currency])}</span></span>\n                    <span className='cmp-details-basic__prices__low-high__gauge'>\n                        <Gauge filled={details.market_data.current_price[currency]} total={details.market_data.high_24h[currency]} />\n                    </span>\n                </div>\n            </div>\n        </div >\n    );\n}\n\nexport default Basic;","/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/navigation/navigation.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/details/chart/chart.js",["132"],"import React from 'react';\n\nimport { Line } from 'react-chartjs-2';\n\nimport './chart.scss';\n\nconst Chart = ({ data, time }) => {\n    const generateDataPoints = () => {\n        let datas = { key: [], value: [] };\n\n        data && data.prices && data.prices.map(d => {\n            datas.key = [...datas.key, formatDate(new Date(d[0]))]\n            datas.value = [...datas.value, d[1].toFixed(d[1] < 10000 ? 4 : 0)]\n        })\n\n        return datas;\n    }\n\n    const formatDate = date => {\n        let res = ''\n\n        switch (time) {\n            case '1':\n                let hours = date.getHours();\n                let minutes = date.getMinutes();\n\n                if (hours < 10) hours = \"0\" + hours;\n                if (minutes < 10) minutes = \"0\" + minutes;\n\n                res = \"\" + hours + \":\" + minutes;\n                break;\n\n            case '7':\n            case '30':\n                const days = ['Sun', 'Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat'];\n\n                res = `${date.getDate()}. ${days[date.getDay()]}`;\n                break;\n\n            case '365':\n                const months = ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'];\n                res = months[date.getMonth()];\n                break;\n\n            case 'max':\n                res = date.getFullYear()\n                break;\n\n            default:\n                break;\n        }\n\n        return res;\n    }\n\n    const chartOptions = {\n        legend: {\n            display: false\n        },\n\n        scales: {\n            yAxes: [\n                {\n                    ticks: {\n                        fontColor: \"white\",\n                        fontStyle: \"bold\"\n                    }\n                }\n            ],\n            xAxes: [\n                {\n                    ticks: {\n                        maxTicksLimit: (() => {\n                            if (time === '1' || time === '7' || '365') return 7;\n                            else if (time === '30') return 16\n                        })(),\n                        fontColor: \"white\",\n                        fontStyle: \"bold\",\n                        maxRotation: 0,\n                        minRotation: 0\n                    }\n                }\n            ]\n        },\n    }\n\n    const chartData = {\n        labels: generateDataPoints().key,\n        datasets: [\n            {\n                borderColor: '#8dc647',\n                pointBorderWidth: 1,\n                pointHoverRadius: 5,\n                pointHoverBackgroundColor: 'rgba(75,192,192,1)',\n                pointHoverBorderColor: 'rgba(220,220,220,1)',\n                pointHoverBorderWidth: 2,\n                pointRadius: 1,\n                pointHitRadius: 10,\n                data: generateDataPoints().value,\n            }\n        ]\n    };\n\n    return (\n        <div className='cmp-details-chart'>\n            <Line data={chartData} options={chartOptions} />\n        </div>\n    );\n}\n\nexport default Chart;","/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/details/gauge/gauge.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/utils/index.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/utils/formatNumbers.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/filter/filter.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/details/description/description.js",["133"],"import React, { useEffect, useState } from 'react';\nimport { FaChevronDown } from 'react-icons/fa';\n\nimport './description.scss';\n\nconst Description = ({ crypto, text }) => {\n    const [hidded, setHidded] = useState(true)\n    const [isBigger, setIsBigger] = useState(false)\n\n    useEffect(() => {\n        getHeightOfDesc()\n    }, [])\n\n    const getHeightOfDesc = () => {\n        const tempEl = document.createElement('div')\n\n        tempEl.innerText = text;\n        document.body.appendChild(tempEl);\n\n        setIsBigger(tempEl.offsetHeight > 150);\n\n        tempEl.remove()\n    }\n\n    return (\n        <div className={`cmp-details-description ${hidded && isBigger ? 'hidded' : ''}`}>\n            <h2 className='cmp-details-description--title'>About {crypto}</h2>\n            <p dangerouslySetInnerHTML={{ __html: text }} className='cmp-details-description--text'></p>\n            { isBigger && <button onClick={() => setHidded(prev => !prev)} className='cmp-details-description--readmore'>\n                <span>{hidded ? 'Read more' : 'Read Less'}</span>\n                <FaChevronDown />\n            </button>}\n        </div>\n    );\n}\n\nexport default Description;","/Use