 className=\"App\">\n      <Router>\n        <Navigation links={[{ path: '/', title: 'Cryptocurrencies' }, { path: '/news', title: 'News' }]} />\n        <Header datas={headerInfos} />\n        <>\n          <Navigation isBurger={false} links={[{ path: '/', title: 'Cryptocurrencies' }, { path: '/news', title: 'News' }]} />\n          <Switch>\n            <Route exact path='/' component={Home} />\n            <Route path='/currencies' component={Currency} />\n            <Route path='/news' component={News} />\n          </Switch>\n        </>\n      </Router>\n    </div >\n  );\n}\n\nexport default App","/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/store/reducers/index.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/api/api.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/pages/News/news.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/header/header.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/pages/Home/home.js",["118"],"import React, { useState, useEffect } from 'react';\nimport { useSelector } from 'react-redux'\nimport { fetchMarkets } from '../../api/api'\nimport Listing from '../../components/listing/listing';\n\nimport './home.scss';\n\nconst Home = () => {\n    const [listingInfos, setListingInfos] = useState([]);\n    const [loadingList, setLoadingList] = useState(false);\n\n    const currencyData = useSelector(state => state.currency)\n\n    useEffect(() => {\n        listinghandler()\n    }, [currencyData]);\n\n    const listinghandler = async () => {\n        setLoadingList(false)\n\n        const listingMarkets = await fetchMarkets(currencyData.currency);\n\n        setLoadingList(true)\n        setListingInfos(listingMarkets);\n    }\n\n    return (\n        <div className='cmp-page-home'>\n            <Listing loadingList={loadingList} keys={['Name', 'Price', '24h', 'Market Cap', 'Volume', 'Circulating Supply', 'Last 7 Days']} datas={listingInfos} currency={currencyData.currency} />\n        </div>\n    );\n}\n\nexport default Home;","/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/listing/listing.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/store/reducers/currencyReducer.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/constants/api.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/currencySelector/currencySelector.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/listing/propTypes.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/store/actions/index.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/store/actions/currencyHandler.js",["119"],"const changeCurrency = currency => {\n    return {\n        type: \"CHANGE_CURRENCY\",\n        payload: currency\n    }\n}\n\nconst fetchCurrencies = currencies => {\n    return {\n        type: \"FETCH_CURRENCIES\",\n        payload: currencies\n    }\n}\n\nexport default {\n    changeCurrency,\n    fetchCurrencies\n}","/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/header/propTypes.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/currencySelector/propTypes.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/commons/loader/loader.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/pages/Currency/currency.js",["120"],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/details/basic.js",["121"],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/navigation/navigation.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/details/chart/chart.js",["122"],"import React from 'react';\n\nimport { Line } from 'react-chartjs-2';\n\nimport './chart.scss';\n\nconst Chart = ({ data, time }) => {\n    const generateDataPoints = () => {\n        let datas = { key: [], value: [] };\n\n        data && data.prices.map(d => {\n            datas.key = [...datas.key, formatDate(new Date(d[0]))]\n            datas.value = [...datas.value, d[1].toFixed(d[1] < 10000 ? 4 : 0)]\n        })\n\n        return datas;\n    }\n\n    const formatDate = date => {\n        let res = ''\n\n        switch (time) {\n            case '1':\n                let hours = date.getHours();\n                let minutes = date.getMinutes();\n\n                if (hours < 10) hours = \"0\" + hours;\n                if (minutes < 10) minutes = \"0\" + minutes;\n\n                res = \"\" + hours + \":\" + minutes;\n                break;\n\n            case '7':\n            case '30':\n                const days = ['Sun', 'Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat'];\n\n                res = `${date.getDate()}. ${days[date.getDay()]}`;\n                break;\n\n            case '365':\n                const months = ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'];\n                res = months[date.getMonth()];\n                break;\n\n            case 'max':\n                res = date.getFullYear()\n                break;\n\n            default:\n                break;\n        }\n\n        return res;\n    }\n\n    const chartOptions = {\n        legend: {\n            display: false\n        },\n\n        scales: {\n            yAxes: [\n                {\n                    ticks: {\n                        fontColor: \"white\",\n                        fontStyle: \"bold\"\n                    }\n                }\n            ],\n            xAxes: [\n                {\n                    ticks: {\n                        maxTicksLimit: (() => {\n                            if (time === '1' || time === '7' || '365') return 7;\n                            else if (time === '30') return 16\n                        })(),\n                        fontColor: \"white\",\n                        fontStyle: \"bold\",\n                        maxRotation: 0,\n                        minRotation: 0\n                    }\n                }\n            ]\n        },\n    }\n\n    const chartData = {\n        labels: generateDataPoints().key,\n        datasets: [\n            {\n                borderColor: '#8dc647',\n                pointBorderWidth: 1,\n                pointHoverRadius: 5,\n                pointHoverBackgroundColor: 'rgba(75,192,192,1)',\n                pointHoverBorderColor: 'rgba(220,220,220,1)',\n                pointHoverBorderWidth: 2,\n                pointRadius: 1,\n                pointHitRadius: 10,\n                data: generateDataPoints().value,\n            }\n        ]\n    };\n\n    return (\n        <div className='cmp-details-chart'>\n            <Line data={chartData} options={chartOptions} />\n        </div>\n    );\n}\n\nexport default Chart;","/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/details/gauge/gauge.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/utils/index.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/utils/formatNumbers.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/filter/filter.js",[],"/Users/sullivantobias/Desktop/Crypto-Tracker/crypto-app/src/components/details/description/description.js",[],{"ruleId":"123","replacedBy":"124"},{"ruleId":"125","replacedBy":"126"},{"ruleId":"127","severity":1,"message":"128","line":23,"column":6,"nodeType":"129","endLine":23,"endColumn":8,"suggestions":"130"},{"ruleId":"127","severity":1,"message":"131","line":16,"column":8,"nodeType":"129","endLine":16,"endColumn":22,"suggestions":"132"},{"ruleId":"133","severity":1,"message":"134","line":15,"column":1,"nodeType":"135","endLine":18,"endColumn":2},{"ruleId":"127","severity":1,"message":"136","line":34,"column":8,"nodeType":"129","endLine":34,"endColumn":24,"suggestions":"137"},{"ruleId":"138","severity":1,"message":"139","li